<v-card-text v-if=\"text\">\r\n                <span class=\"white--text\">{{text}}</span>\r\n            </v-card-text>\r\n        </v-card>\r\n    </v-snackbar>\r\n</template>\r\n\r\n<script>\r\n    export default {\r\n        name: 'AlertMessage',\r\n        data() {\r\n            return {\r\n                snackbar: false,\r\n                x: '',\r\n                y: '',\r\n                color: '',\r\n                mode: '',\r\n                icon: null,\r\n                title: '',\r\n                text: '',\r\n                timeout: 6000\r\n            }\r\n        },\r\n        methods: {\r\n            showAlertSimple(color, title) {\r\n                this.color = color;\r\n                this.title = title;\r\n                this.x = \"right\";\r\n                this.y = \"bottom\";\r\n                if (color === \"success\") {\r\n                    this.icon = 'check_circle';\r\n                } else if (color === \"error\") {\r\n                    this.icon = 'close';\r\n                } else if (color === \"info\") {\r\n                    this.icon = 'info';\r\n                } else if (color === \"warning\") {\r\n                    this.icon = 'warning';\r\n                }\r\n                this.text = '';\r\n                this.mode = '';\r\n                this.timeout = 6000;\r\n                this.snackbar = true;\r\n            },\r\n            /**\r\n             * Show the alert with all configuration options\r\n             * @param icon Alert's icon\r\n             * @param color Alert's color: success, error, info, primary, warning\r\n             * @param title mode: vertical, milti-line or empty string '' for center\r\n             * @param title timeout: timeout to disappear the alert: top or bottom\r\n             * @param title text Alert text\r\n             */\r\n            showAlertFull(icon, color, title, mode, timeout, x, y, text = null) {\r\n                this.icon = icon;\r\n                this.color = color;\r\n                this.text = text;\r\n                this.mode = mode;\r\n                this.timeout = timeout;\r\n                this.x = x;\r\n                this.y = y;\r\n                this.snackbar = true;\r\n                this.title = title;\r\n            }\r\n        }\r\n    }\r\n</script>\r\n\r\n<style scoped>\r\n     .v-snack__content {\r\n        padding: 0 !important;\r\n    }\r\n</style\r\n ","<template> \r\n    <v-dialog id=\"modalRemove\" max-width=\"500px\" v-model=\"dialog\">\r\n        <v-card>\r\n            <v-card-title class=\"headline\">\r\n                <div class=\"title\">\r\n                    Eliminar {{modalData.typeElement}}\r\n                </div>\r\n            </v-card-title>\r\n            <v-card-text>\r\n                ¿Estas seguro de eliminar {{modalData.preposition}}\r\n                {{modalData.typeElement}}\r\n                {{modalData.mainNameElement}}?\r\n            </v-card-text>\r\n            <v-card-actions>\r\n                <v-spacer></v-spacer>\r\n                <v-btn color=\"default\" text v-on:click=\"cancel\">\r\n                    Cancelar\r\n                </v-btn>\r\n                <v-btn color=\"error\" depressed @click=\"removeElement\">\r\n                    Eliminar\r\n                </v-btn>\r\n            </v-card-actions>\r\n        </v-card>\r\n    </v-dialog>\r\n</template>\r\n\r\n<script>\r\n    export default {\r\n        name: \"ModalRemove\",\r\n        props: ['modalData'],\r\n        data() {\r\n            return {\r\n                dialog: false\r\n            }\r\n        },\r\n        methods: {\r\n            removeElement() {\r\n                this.$emit('id_element', this.modalData._id);\r\n                this.dialog = false;\r\n            },\r\n            cancel() {\r\n                this.dialog = false;\r\n            }\r\n        },\r\n    }\r\n</script>\r\n\r\n<style>\r\n\r\n</style>","<template>\r\n    <v-form >\r\n        <v-card-title>\r\n            <div class=\"subtitle-2\">\r\n                DATOS GENERALES\r\n            </div>\r\n        </v-card-title>\r\n        <v-col>\r\n            <v-card-text>\r\n                <v-text-field v-model=\"user.name\" id=\"inputName\" name=\"name\" disabled \r\n                            label=\"Nombre completo\">\r\n                </v-text-field>\r\n                <v-text-field v-model=\"user.username\" id=\"inputUsername\" name=\"username\" disabled           \r\n                            label=\"Usuario\">\r\n                </v-text-field>\r\n                <v-text-field v-model=\"user.email\" id=\"inputEmail\" name=\"email\" disabled \r\n                            label=\"Correo electronico\">\r\n                </v-text-field>\r\n                <v-text-field v-model=\"user.profile\" id=\"inputProfile\" name=\"profile\" disabled \r\n                            label=\"Perfil\">\r\n                </v-text-field>\r\n            </v-card-text>\r\n        </v-col>\r\n    </v-form>\r\n</template>\r\n\r\n<script>\r\n    export default {\r\n        name: \"GeneralData\",\r\n        data() {\r\n            return {\r\n                user: {\r\n                    _id: null,\r\n                    name: null,\r\n                    username: null,\r\n                    email: null,\r\n                    profile: null\r\n                }\r\n            }\r\n        },\r\n    }\r\n</script>\r\n\r\n<style>\r\n\r\n</style>","<template>\r\n    <v-form @submit.prevent=\"updatePassword\">\r\n        <v-card>\r\n            <v-card-title>\r\n                <div class=\"subtitle-2\">CAMBIO DE CONTRASEÑA</div>\r\n            </v-card-title>\r\n            <v-card-text>\r\n                <v-text-field v-model=\"password.old\" id=\"inputPassword\"\r\n                            :append-icon=\"showPass.old?'mdi-eye':'mdi-eye-off'\"\r\n                            :type=\"showPass.old?'text':'password'\"\r\n                            name=\"current_password\"\r\n                            label=\"Contraseña actual\"\r\n                            @click:append=\"showPass.old= !showPass.old\"\r\n                            autocomplete=\"off\">\r\n                </v-text-field>\r\n                <v-text-field v-model=\"password.new\" id=\"inputNewPassword\"\r\n                            :append-icon=\"showPass.new?'mdi-eye':'mdi-eye-off'\"\r\n                            :type=\"showPass.new?'text':'password'\"\r\n                            name=\"password\"\r\n                            label=\"Nueva contraseña\"\r\n                            @click:append=\"showPass.new= !showPass.new\"\r\n                            autocomplete=\"new password\">\r\n                </v-text-field>\r\n                <v-text-field v-model=\"password.confirm\" id=\"inputConfirmPassword\"\r\n                            :append-icon=\"showPass.confirm?'mdi-eye':'mdi-eye-off'\"\r\n                            :type=\"showPass.confirm?'text':'password'\"\r\n                            name=\"password_confirmation\"\r\n                            label=\"Confirmar contraseña\"\r\n                            @click:append=\"showPass.confirm= !showPass.confirm\">\r\n                </v-text-field>\r\n                <!-- DAMOS MEJOR UBICACION AL BOTON -->\r\n                <v-card-actions>\r\n                    <v-row justify=\"center\">\r\n                        <v-btn type=\"submit\" color=\"primary\" rounded depressed>Cambiar</v-btn>\r\n                    </v-row>\r\n                </v-card-actions>\r\n            </v-card-text>\r\n        </v-card>\r\n    </v-form>\r\n</template>\r\n\r\n<script>\r\n    export default {\r\n        name: \"UpdatePassword\",\r\n        data() {\r\n            return {\r\n                password: {\r\n                    old: null,\r\n                    new: null,\r\n                    confirm: null\r\n                },\r\n                showPass: {\r\n                    old: null,\r\n                    new: null,\r\n                    confirm: null    \r\n                }\r\n            }\r\n        },\r\n        methods: {\r\n            updatePassword() {\r\n            }\r\n        },\r\n    }\r\n</script>\r\n\r\n<style>\r\n\r\n</style>","<template>\r\n    <v-menu offset-y>\r\n        <template v-slot:activator=\"{on}\">\r\n            <v-btn color=\"default\" dark text v-on=\"on\" class=\"mr-5\">\r\n                Ruben\r\n                <v-icon>keyboard_arrow_down</v-icon>\r\n            </v-btn>\r\n        </template>\r\n        <v-list>\r\n            <v-list-item href=\"#\" :to=\"{name:'home.account'}\">Cuenta</v-list-item>\r\n            <v-list-item href=\"#\">Cerrar sesion</v-list-item>\r\n        </v-list>\r\n    </v-menu>\r\n</template>\r\n\r\n<script>\r\n    export default {\r\n        name: \"UserLogged\"\r\n    }\r\n</script>\r\n\r\n<style>\r\n\r\n</style>","<template>\r\n    <v-footer padless>\r\n        <v-col class=\"d-flex justify-center\" cols=\"12\">\r\n            <span class=\"white--text\">\r\n                DFE Developer Front-End\r\n            </span>\r\n        </v-col>\r\n    </v-footer>\r\n</template>\r\n\r\n<script>\r\n    export default {\r\n        name: \"FooterView\"\r\n    }\r\n</script>\r\n\r\n<style scoped>\r\n    footer {\r\n        position: absolute;\r\n        bottom: 0;\r\n        left: 0;\r\n        width: 100vw;\r\n        height: 50px;\r\n        background-color: #8c191d !important;\r\n    }\r\n</style>","<template>\r\n    <v-app-bar app dark dense src=\"https://cdn.vuetifyjs.com/images/backgrounds/vbanner.jpg\">\r\n        <v-toolbar-title>Scaffold Meteor + Vue</v-toolbar-title>\r\n        <v-spacer></v-spacer>\r\n        <user-logged></user-logged>\r\n        <template v-slot:extension>\r\n            <v-tabs v-model=\"optionSelected\" align-with-title>\r\n                <v-tab v-for=\"option in options\" :key=\"option.title\" :to=\"{name:option.nameRoute}\"      \r\n                        v-text=\"option.title\">\r\n                </v-tab>\r\n            </v-tabs>\r\n        </template>\r\n    </v-app-bar>\r\n</template>\r\n\r\n<script>\r\n    import UserLogged from '../../components/UserLogged/UserLogged';\r\n    \r\n    export default {\r\n        name: \"HeaderView\",\r\n        components: {\r\n            UserLogged\r\n        },\r\n        data() {\r\n            return {\r\n                optionSelected: 0,\r\n                options: [\r\n                    {\r\n                        icon: 'home',\r\n                        title: 'Inicio',\r\n                        nameRoute: 'home'\r\n                    },\r\n                    {\r\n                        icon: 'person',\r\n                        title: 'Usuarios',\r\n                        nameRoute: 'home.users'\r\n                    },\r\n                    {\r\n                        icon: 'user-tag',\r\n                        title: 'Perfiles',\r\n                        nameRoute: 'home.profiles'\r\n                    }\r\n                ]\r\n            }\r\n        },\r\n    }\r\n</script>\r\n\r\n<style>\r\n\r\n</style>","<template>\r\n    <v-row>\r\n        <v-col xs=\"12\" sm=\"8\" md=\"4\" class=\"d-flex flex-column justify-space-around pa-12\">\r\n            <div class=\"text-center\">\r\n                <img src=\"./img/vuetify.png\" alt=\"Vuetify\" height=\"180px\" width=\"auto\" />\r\n            </div>\r\n            <router-view name=\"sectionView\"></router-view>\r\n            <div class=\"text-center\">\r\n                <img src=\"./img/Powered.png\" alt=\"Diseño\" id=\"poweredLogo\" height=\"35px\" />\r\n            </div>\r\n        </v-col>\r\n        <v-col xs=\"12\" sm=\"4\" md=\"8\" class=\"right-side d-flex flex-column justify-center\">\r\n            <div class=\"display-3 font-weight-medium mr-10 text-right white--text\">\r\n                Scaffold Meteor + Vue\r\n            </div>\r\n        </v-col>\r\n    </v-row>\r\n</template>\r\n\r\n<script>\r\n    export default {\r\n        name: \"LytAuth\"\r\n    }\r\n</script>\r\n \r\n<style scoped>\r\n    .display-3 {\r\n        z-index: 2;\r\n        margin-top: 350px;\r\n    }\r\n\r\n    .right-side{\r\n        background-image: url(\"./img/meteor.jpg\");\r\n        background-size: cover;\r\n        color: var(--body-color);\r\n        height: 100vh;\r\n        position: relative;\r\n    }\r\n</style>\r\n","<template>\r\n    <v-container fluid>\r\n        <header-view></header-view>\r\n        <v-main id=\"main_section\">\r\n            <router-view name=\"sectionView\"></router-view>\r\n        </v-main>\r\n        <footer-view></footer-view>\r\n    </v-container>\r\n</template>\r\n\r\n<script>\r\nimport HeaderView from './shared/HeaderView';\r\nimport FooterView from './shared/FooterView';\r\n\r\n    export default {\r\n        name: \"LytAuth\",\r\n        components: {\r\n            HeaderView,\r\n            FooterView\r\n            \r\n        }\r\n    }\r\n</script>\r\n\r\n<style scoped>\r\n    #main_section {\r\n        margin-bottom: 80px;\r\n        position: relative;\r\n    }\r\n</style>","<template>\r\n    <v-container>\r\n        <v-row>\r\n            <v-col>\r\n                <div class=\"headline\">Configurar cuenta</div>\r\n            </v-col>\r\n        </v-row>\r\n        <v-row>\r\n            <v-col cols=\"12\" xs=\"12\" sm=\"12\" md=\"6\" lg=\"6\" xl=\"5\">\r\n                <general-data />\r\n            </v-col> \r\n            <v-col cols=\"12\" xs=\"12\" sm=\"12\" md=\"6\" lg=\"6\" offset-xl=\"1\" xl=\"5\">\r\n                <update-password />\r\n            </v-col>\r\n        </v-row>\r\n    </v-container>\r\n</template>\r\n\r\n<script>\r\n    import GeneralData from '../../components/ConfigureAccount/GeneralData';\r\n    import UpdatePassword from '../../components/ConfigureAccount/UpdatePassword';\r\n    \r\n    export default {\r\n        name: \"ConfigureAccount\",\r\n        components: {\r\n            GeneralData,\r\n            UpdatePassword\r\n        }\r\n    }\r\n</script>\r\n\r\n<style>\r\n\r\n</style>","<template>\r\n    <div>\r\n        <div class=\"d-flex flex-row justify-start\">\r\n            <v-btn color=\"primary\" icon :to=\"{name:'login'}\">\r\n                <v-icon>arrow_back</v-icon>\r\n            </v-btn>\r\n            <div class=\"title\">Olvidé mi contraseña</div>\r\n        </div>\r\n        <v-form @submit.prevent=\"forgotPassword\">\r\n            <v-text-field v-model=\"user.email\" id=\"inputEmail\" name=\"email\" type=\"email\"\r\n                            label=\"Correo electronico\">\r\n\r\n            </v-text-field>\r\n            <v-btn type=\"submit\" color=\"primary\" rounded>Recuperar</v-btn>\r\n        </v-form>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\n    export default {\r\n        name: \"ForgotPassword\",\r\n        data() {\r\n            return {\r\n                user: {\r\n                    email: null\r\n                }\r\n            }\r\n        },\r\n        methods: {\r\n            forgotPassword() {\r\n                console.log(\"user:\", this.user);\r\n            }\r\n        },\r\n    }\r\n</script>\r\n\r\n<style>\r\n\r\n</style>","<template>\r\n    <div class=\"login-wrapper\">\r\n        <div class=\"tittle secondary--text\">Bienvenido!</div>\r\n        <div class=\"display-1 mb-0 secondary--text\">Iniciar sesión</div>\r\n        <v-form @submit.prevent=\"Login\" aria-autocomplete=\"none\">\r\n            <v-text-field id=\"inputUser\" v-model=\"user.userOrEmail\" autocompete=\"off\"\r\n                    label=\"Usuario\" name=\"email\" prepend-icon=\"person\" color=\"primary\" type=\"text\">\r\n            </v-text-field>\r\n            <v-text-field id=\"inputPassword\" label=\"Contraseña\" name=\"password\" \r\n                    prepend-icon=\"lock\" v-model=\"user.password\" type=\"password\">                \r\n            </v-text-field>\r\n            <div class=\"d-flex justify-end\">